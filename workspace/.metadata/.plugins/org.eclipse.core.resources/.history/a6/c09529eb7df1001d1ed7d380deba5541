 SnmpAPI.SET_REQ_MSG, SnmpAPI.TRP_REQ_MSG,
 SnmpAPI.GET_RSP_MSG,
 SnmpAPI.TRP2_REQ_MSG, SnmpAPI.GETBULK_REQ_MSG,
 SnmpAPI.INFORM_REQ_MSG and SnmpAPI.REPORT_MSG
